<?xml version='1.0' encoding='utf-8'?>
<widget id="com.davidsvane.mt" version="1.1.9d" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MusicalTeatret</name>
    <description>
        MusicalTeatrets Officielle App
    </description>
    <author email="davidsvane@yahoo.dk" href="https://www.facebook.com/davidsvane">
        David Svane
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="http://*/*" launch-external="yes" />
    <allow-intent href="https://*/*" launch-external="yes" />
    <allow-navigation href="http://www.musicalteatret.dk/*" />
    <allow-navigation href="http://www.davidsvane.com/*" />
    <allow-navigation href="*youtube*" />
    <allow-navigation href="*ytimg*" />
    <allow-navigation href="*youtube-nocookie*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
</widget>
